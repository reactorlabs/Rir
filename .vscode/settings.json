{
  "git.ignoreLimitWarning": true, // custom-r
  "C_Cpp.formatting": "Disabled",
  "files.insertFinalNewline": false,
  "files.trimFinalNewlines": false,
  "files.trimTrailingWhitespace": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "external/gnur": true,
    "external/custom-r/include": true,
    "**/*.d": true,
    "**/*.ts": true,
    "**/*.o": true
  },
  "files.associations": {
    "*.ml": "ocaml",
    "*.mli": "ocaml",
    "iosfwd": "cpp",
    "__locale": "cpp",
    "ios": "cpp",
    "__functional_03": "cpp",
    "__config": "cpp",
    "functional": "cpp",
    "queue": "cpp",
    "stack": "cpp",
    "__hash_table": "cpp",
    "__tree": "cpp",
    "algorithm": "cpp",
    "filesystem": "cpp",
    "forward_list": "cpp",
    "list": "cpp",
    "*.tcc": "cpp",
    "chrono": "cpp",
    "deque": "cpp",
    "map": "cpp",
    "set": "cpp",
    "string": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "vector": "cpp",
    "iterator": "cpp",
    "random": "cpp",
    "cmath": "cpp"
  }
}
